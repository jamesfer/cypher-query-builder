{
  "name": "cypher-query-builder",
  "version": "0.1.0",
  "description": "An intuitive, easy to use query builder for Neo4j and Cypher",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha src/*.spec.js src/**/*.spec.js"
  },
  "keywords": [
    "cypher",
    "query",
    "builder",
    "neo4j"
  ],
  "author": "James Ferguson",
  "license": "MIT",
  "dependencies": {
    "chai-as-promised": "^7.0.0",
    "lodash": "^4.17.4",
    "neo4j-driver": "^1.3.0",
    "node-cleanup": "^2.1.2"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3"
  },
  "nyc": {
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.tests.js"
    ]
  }
}
